# MT5 Windows Bridge Configuration
# Copy this file to .env and fill in your values

# ===========================================
# MT5 Configuration (Windows)
# ===========================================
MT5_ACCOUNT=12345678
MT5_PASSWORD=your_password
MT5_SERVER=Tickmill-Demo

# Optional: Full path to MT5 terminal (leave empty for auto-detection)
# MT5_PATH=C:\Program Files\MetaTrader 5\terminal64.exe

# ===========================================
# Linux Server Database (PostgreSQL)
# ===========================================
POSTGRES_HOST=192.168.1.100  # IP of your Linux server
POSTGRES_PORT=5432
POSTGRES_DB=trading_db
POSTGRES_USER=trading_user
POSTGRES_PASSWORD=your_generated_password

# ===========================================
# Linux Server Cache (Redis)
# ===========================================
REDIS_HOST=192.168.1.100  # IP of your Linux server
REDIS_PORT=6379
REDIS_DB=0

# ===========================================
# Trading Configuration
# ===========================================
TRADING_PAIRS=EURUSD,GBPUSD,USDJPY,AUDUSD,USDCAD
TRADING_TIMEFRAME=M15

# ===========================================
# Logging
# ===========================================
LOG_LEVEL=INFO
